<div id="admin_publications">
  <div class="margin_top_bottom_one center">
    <h2>Admin Users</h2>
  </div>
  <div class="block">
    <%= link_to("Add New Admin User", new_admin_user_path, :class => 'blue_btn shadow new centered') %>
  </div>
  <div class="shadow">
    <% @admin_users.each do |admin_user| %>
    <div class="ninety_five padding_half text_left <%= cycle('white_background', 'gray_background') %>">
      <div class="seventy inline">
        <p class="inline gray margin_none"><span class="bold"><%= admin_user.first_name %> <%= admin_user.last_name %></span> -<span class="italic underline"> <%= mail_to(admin_user.email) %></span></p>
      </div>
      <div class="float_right forty">
        <%= link_to("delete", delete_admin_user_path(admin_user), :class => 'action delete inline float_right') %>
        <%= link_to("edit", edit_admin_user_path(admin_user), :class => 'action edit inline float_right margin_right_one') %>
      </div>
    </div>
    <% end %>
</div>
