<div id="admin_publications">
  <div class="margin_top_bottom_one center">
    <h1>Add a new RAMP Measure</h1>
  </div>

  <%= form_for @ramp_pages, url: ramp_pages_path, html: { multipart: true}  do |f| %>

  <%= render(:partial => 'form', :locals => {:f => f}) %>

  <div class="form_buttons">
    <%= f.submit("save", :class => "blue_btn shadow margin_top_one block centered", :style => "color: #fff; border: none; cursor: pointer; font-size: 1.2em;") %>
  </div>

  <% end %>
</div>
